{
    "name": "timwassenburg/service-generator",
    "description": "Generate Laravel services",
    "keywords": ["generator", "php", "cli", "laravel", "artisan", "services", "pattern"],
    "homepage": "https://github.com/timwassenburg/service-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Wassenburg",
            "email": "timwass@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/console": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "TimWassenburg\\ServiceGenerator\\": "src/",
            "TimWassenburg\\ServiceGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TimWassenburg\\ServiceGenerator\\Http\\Providers\\CommandServiceProvider",
                "TimWassenburg\\ServiceGenerator\\ServiceGeneratorServiceProvider"
            ]
        }
    }
}
